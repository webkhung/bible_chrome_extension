{
  "name": "Bible Reading Plans",
  "description": "Select your favorite bible reading plans, read the verse whenever you open a new tab, play simple games to memorize the verses.",
  "permissions": [
    "storage",
    "http://biblereadingplans.herokuapp.com/verses",
    "http://localhost:3000/verses",
    "http://localhost:3001/verses"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "version": "0.8.6",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "web_accessible_resources": [
    "reading_plan.js",
    "jqmeter.min.js",
    "circle-progress.js"
  ],
  "manifest_version": 2
}
